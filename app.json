{
  "expo": {
    "name": "Pharmaconnect",
    "slug": "pharmaconnect",
    "scheme": "pharmaconnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/image1.png",
      "resizeMode": "contain",
      "backgroundColor": "#95d5b2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Pharmaconnect.Mobile"
    },
    "android": {
      "package": "com.Pharmaconnect.Mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5aef722b-c9b4-4409-bf4b-221630d1c17d"
      }
    },
    "owner": "aymen723",
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Pharmaconnect to use your location."
        }
      ]
    ]
  }
}
